<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"/>
    <link rel="stylesheet" href="/static/file-upload-with-preview.min.css"/>
    <link rel="stylesheet" href="https://onesignal.github.io/emoji-picker/lib/css/emoji.css">
    <link rel="stylesheet" href="/static/style.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  </head>
  <body>
    <div class="geek-combat chat-app">
      <div class="left-wrap">
        <% friends = data.online_users %>
        <%- include('../partials/header', {appData: appData, userDetails: data.userDetails}); %>
        <div class="wrapper">
          <div class="container-search">
            <div class="container-input">
              <input type="text" list="psychology_theories_list" id="newers_search" class="search-term" placeholder="Search on newers name">
              <button type="button" class="search-button" id="selectTheory">
                <i class="fa fa-search"></i>
             </button>
            </div>
          </div>
        </div>
        <%- include('../partials/friendList', {friends: friends, onlineUsers: data.online_users}); %>
      </div>
      <div class="right-wrap">
        <%- include('../partials/chatInterface', {friend: {friends}, chats:
        []}); %>
      </div>
    </div>
    <form id="entry-form" style="display:none" method="post" enctype="multipart/form-data"></form>
  </body>
  <script src="/socket.io/socket.io.js"></script>
  <script src="https://onesignal.github.io/emoji-picker/lib/js/config.js"></script>
  <script src="https://onesignal.github.io/emoji-picker/lib/js/util.js"></script>
  <script src="https://onesignal.github.io/emoji-picker/lib/js/jquery.emojiarea.js"></script>
  <script src="https://onesignal.github.io/emoji-picker/lib/js/emoji-picker.js"></script>
  <script src="/js/index.js"></script>
</html>
